{
  "pages": [
    "pages/home/home",
    "pages/logs/logs"
  ],
  "subPackages": [
    {
      "root": "pages/logsAndList/",
      "name": "other",
      "pages": [
        "applist/applist"
      ]
    },
    {
      "root": "pages/video/",
      "name": "video",
      "pages": [
        "removeLogo/removeLogo",
        "videoInfo/videoInfo"
      ]
    },
    {
      "root": "pages/text/",
      "name": "text",
      "pages": [
        "text"
      ]
    }
  ],
  "preloadRule": {
    "pages/home/home": {
      "network": "all",
      "packages": [
        "video",
        "other",
        "text"
      ]
    }
  },
  "window": {
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#FFFFFF",
    "navigationBarTitleText": "雨非工具",
    "navigationBarTextStyle": "black",
    "navigationStyle": "custom",
    "initialRenderingCache": "dynamic"
  },
  "usingComponents": {},
  "tabBar": {
    "color": "#aaaaaa",
    "borderStyle": "white",
    "backgroundColor": "#fff",
    "selectedColor": "#0081ff",
    "list": [
      {
        "pagePath": "pages/home/home",
        "iconPath": "/static/tab_icon/home.png",
        "selectedIconPath": "/static/tab_icon/home_cur.png",
        "text": "首页"
      },
      {
        "pagePath": "pages/logs/logs",
        "iconPath": "/static/tab_icon/logs.png",
        "selectedIconPath": "/static/tab_icon/logs_cur.png",
        "text": "历史记录"
      }
    ]
  },
  "darkmode": true,
  "lazyCodeLoading": "requiredComponents",
  "sitemapLocation": "sitemap.json"
}
